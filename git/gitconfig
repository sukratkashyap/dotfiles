[alias]
    ch = checkout
    br = branch
    cm = commit
    cma = commit --amend
    st = status
    rsh = reset --soft HEAD~1
    rhh = reset --hard HEAD~1
    lgs = log --pretty --oneline
[push]
    default = current
[pull]
    ff = only
    rebase = true
